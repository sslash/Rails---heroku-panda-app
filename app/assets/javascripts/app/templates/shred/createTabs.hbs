
<p class="lead">Create Tabs</p>
<video class="video-js vjs-default-skin battle-video" controls>
	<source src="uploads/{{videoPath}}" type='video/mp4'>
</video>

<div class="row-fluid">
	<div class="span3">
		<p><li class="icon-music icon-large"></li> = <span>120</span></p>
	</div>

	<div class="span6">
		<img src="images/icons/notes/helnote.png" class="notes" id="semibreve">
		<img src="images/icons/notes/halvnote.png"class="notes" id="minim">
		<img src="images/icons/notes/firedels.png"class="notes selected" id="crotchet">
		<img src="images/icons/notes/attedels.png"class="notes" id="quaver">
		<img src="images/icons/notes/sekstendels.png"class="notes" id="semiquaver">
		<img src="images/icons/notes/tretitodels.png"class="notes" id="demisemiquaver">
		<img src="images/icons/notes/64dels.png"class="notes" id="hemidemisemiquaver">
	</div>


	<div class="span3">
		<img src="images/icons/notes/hvilepause.png"class="notes">
	</div>

</div>

<div class="row-fluid">
	<div class="span1"> <li class="icon-chevron-left icon-2x"></li></div>
	<div class="span10">
	<div class="tab">
		<canvas id="bars" style="width:100%; height:60px"></canvas>
		<input type="text" id="tab-input" autofocus></input>
	</div>
</div>
	<div class="span1"><li class="icon-chevron-right icon-2x"></li> </div>
</div>
